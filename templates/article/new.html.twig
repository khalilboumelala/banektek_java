        <script>
            function fillCountryDropdown() {
                // Faire une requ√™te √† l'API Restcountries
                $.ajax({
                    url: 'https://restcountries.com/v3.1/all',
                    method: 'GET',
                    success: function (data) {
                        // Parcourir les donn√©es et ajouter chaque pays au menu d√©roulant
                        data.forEach(function (country) {
                            // Ignorer la Tunisie car elle est d√©j√† la premi√®re option
                            if (country.alpha2Code !== 'tn') {
                                $('#countryDropdown').append('<option value="' + country.alpha2Code + '">' + country.name.common + '</option>');
                            }
                        });
                    },
                    error: function (error) {
                        console.error('Erreur lors de la r√©cup√©ration des pays', error);
                    }
                });
            }

            // Appeler la fonction pour remplir le menu d√©roulant au chargement de la page
            fillCountryDropdown();
        </script>
